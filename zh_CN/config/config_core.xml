<?xml version="1.0" encoding="utf-8"?>
<Config>
	<!-- 版本号 -->
	<version>0.0.1.0</version>
	
	<!-- 跨域策略文件路径 不写表示根据flashplayer自动读取-->
	<PolicyPath></PolicyPath>
	
	<!-- 垃圾回收时间间隔（秒），如果为0则表示不进行垃圾回收，垃圾回收包括清空对象池缓存对象 -->
	<GCInterval>600</GCInterval>
	
	<!-- Tip所需数据，包括持续的秒数和Tip跟随鼠标时的偏移量  -->
	<Tip lastSeconds="5" offsetX="15" offsetY="15"/>
	
	<!-- 显示人人LOGO 1 显示 0 不显示-->
<!--	<Logo>1</Logo> -->
	<!-- 战斗版本号，0不使用，1使用 -->
	<BattleVersion>0</BattleVersion>
	
	<!-- 游戏中字体设置,都保存到TextFontType中 -->
	<HEITI>黑体</HEITI>
	<SONGTI>宋体</SONGTI>
	<LISHU>隶书</LISHU>
	<KAITI>楷体_GB2312</KAITI>
	
	<!-- 游戏中CoreTextField的初始化文字样式。可以使用CoreTextField.setStyle方法切换设置好的样式，默认是第一个样式 -->
  <TextFormat font="宋体" size="12" color="0xffffff" glow="true" glowColor="0x000000" bold="false" italic="false" underline="false" type="default" comment="默认文字" />  
  <TextFormat font="宋体" size="14" color="0xffffff" glow="true" glowColor="0x744010" bold="false" italic="false" underline="false" type="mainfacenormal" comment="主界面face正常状态文字" />
